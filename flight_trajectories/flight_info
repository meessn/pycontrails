     return_flight=True
 )

 london - istanboel (a320neo):


 flight = opensky.history(
    "2025-01-09 18:00",
    stop="2025-01-09 22:00",
    departure_airport="EGLL",  # London Heathrow
    arrival_airport="LTFM",
    callsign = "BAW680"         # Istanbul
)
specific_callsign = "BAW680"

